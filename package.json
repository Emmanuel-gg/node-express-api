{
  "name": "web-api",
  "version": "1.0.0",
  "description": "Api in Node 18",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:init": "node ./database/scripts/init.js",
    "test": "jest --watchAll --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:unit": "jest --watchAll --testPathPattern=src/js/tests/unit-tests --watchAll --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:integration": "jest --watchAll --testPathPattern=src/js/tests/integration --watchAll --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:helpers": "jest --watchAll jest --findRelatedTests src/js/tests/unit-tests/helpers/helpers.test.js --watchAll --no-cache  --detectOpenHandles --runInBand --forceExit"
  },
  "author": "Emmanuel",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "eslint": "8.34.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "nodemon": "2.0.20",
    "sequelize": "6.28.0",
    "sequelize-cli": "6.6.0"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "mysql2": "3.1.2"
  }
}
